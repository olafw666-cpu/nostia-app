{
  "name": "nostia-backend",
  "version": "1.0.0",
  "description": "Nostia social adventure app backend - connecting travelers and adventurers",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "audit": "npm audit --production",
    "audit:fix": "npm audit fix --production",
    "backup": "node -e \"const fs=require('fs');const d=new Date().toISOString().slice(0,10);fs.copyFileSync('database/nostia.db','database/backups/nostia-'+d+'.db');console.log('Backup created: nostia-'+d+'.db')\"",
    "prestart": "npm audit --production --audit-level=high || true"
  },
  "keywords": [
    "travel",
    "social",
    "adventure",
    "trip-planning",
    "mongodb",
    "express"
  ],
  "author": "olaf woodall",
  "license": "MIT",
  "dependencies": {
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/stack": "^7.6.13",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "expo": "^54.0.30",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3",
    "node-fetch": "^2.7.0",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.19.0",
    "sqlite3": "^5.1.7",
    "stripe": "^14.25.0"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "nodemon": "^3.0.1",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
